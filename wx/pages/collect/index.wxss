.collect_main {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #f3f4f6;
}

.goods_item {
  position: relative;
  width: 96%;
  left: 2%;
  height: 100px;
  border-style: solid;
  border-color: rgb(156, 149, 138);
  margin-top: 2%;
  border-radius: 10px;
  background-color: rgb(247, 224, 191);
}



.goods_img {
  position: relative;
  width: 70px;
  height: 70px;
  left: 5px;
  top: 15px;
  border-radius: 50px;
}

.goods_name {
  position: absolute;
  width: 75%;
  height: 25px;
  left: 22%;
  top: 10%;
  /* border-style: solid; */
  font-size: 14px;
  border-radius: 5px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-weight: 800;
}

.goods_price {
  position: absolute;
  width: 75%;
  height: 55px;
  left: 22%;
  top: 40%;
  /* border-style: solid; */
  font-size: 12px;
  border-radius: 5px;
  overflow: hidden;
  text-overflow: ellipsis;
  font-family: '幼圆';
}